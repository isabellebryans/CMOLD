@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:   <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://rsp4j.io/covid/observations/>.
@prefix shape: <http://rsp4j.io/covid/shapes/>.
@prefix sh: <http://www.w3.org/ns/shacl#> .


# We DONT want a Facebook post in the stream
# If a Facebook Post occurs in the stream, it causes a SHACL violation
# Conforms if NO facebook posts received in stream
# Can be applied at stream level or after windowing


shape:notFBpost
    a sh:NodeShape ;
    sh:targetClass :FacebookPost ;
    sh:property [
                sh:path rdf:type;
                sh:maxCount 0;
                ].


