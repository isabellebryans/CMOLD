@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:   <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://rsp4j.io/covid/observations/>.
@prefix shape: <http://rsp4j.io/covid/shapes/>.
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix tracing: <http://rsp4j.io/covid/tracing/>.

shape:ObservationShape
    a sh:NodeShape;
    sh:targetSubjectsOf rdf:type;
    sh:or ( [ sh:path rdf:type ;
              sh:hasValue :FacebookPost;
            ]
            [ sh:path rdf:type ;
              sh:hasValue :RFIDObservation;
            ]
      ).
