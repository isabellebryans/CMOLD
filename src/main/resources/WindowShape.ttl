@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs:   <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://rsp4j.io/covid/observations/>.
@prefix shape: <http://rsp4j.io/covid/shapes/>.
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix person: <http://rsp4j.io/covid/people/>.

shape:FacebookPostShape1
    a sh:NodeShape ;
    sh:targetClass :FacebookPost;
    sh:property [
          sh:path :who;
          sh:minCount 1;
      ];
    sh:property [
          sh:path :where;
          sh:minCount 1;
          sh:maxCount 1;
      ].

shape:PersonShape
    a sh:NodeShape ;
    sh:targetObjectsOf :who;
    sh:property [
                sh:path :isIn;
                sh:maxCount 1].
